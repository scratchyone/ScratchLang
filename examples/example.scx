sprite Dog {
    function varstuff() {
        var ret = "epic return value"
        return ret
    }
    function badstuff() {
        return Dog.varstuff()
    }
    function main() {
        const message = "Will now show return value:"
        say(message, 3)
        say(Dog.badstuff(), 2)
        say("Displayed!")
    }
}